<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/madMeal_name1"
        android:layout_width="96dp"
        android:layout_height="70dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/madMeal_age1"
        android:layout_width="119dp"
        android:layout_height="82dp"
        android:layout_marginTop="20dp"
        android:text="Age"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/madMeal_hist1"
        android:layout_width="327dp"
        android:layout_height="347dp"
        android:layout_marginTop="30dp"
        android:text="Historik(startstid, varighed, type, afstand)"
        app:layout_constraintTop_toBottomOf="@+id/madMeal_bruger"
        tools:layout_editor_absoluteX="31dp" />

    <TextView
        android:id="@+id/madMeal_bruger"
        android:layout_width="308dp"
        android:layout_height="93dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="52dp"
        android:text="Angiv mad(brugerdefineret)"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/madMeal_name1" />
</android.support.constraint.ConstraintLayout>