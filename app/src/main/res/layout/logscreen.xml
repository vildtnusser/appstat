<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/log_name1"
        android:layout_width="126dp"
        android:layout_height="85dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/log_sleep1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.012" />

    <TextView
        android:id="@+id/log_age1"
        android:layout_width="136dp"
        android:layout_height="87dp"
        android:layout_marginTop="20dp"
        android:text="Age"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/log_sleep1"
        android:layout_width="84dp"
        android:layout_height="45dp"
        android:layout_marginStart="159dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="168dp"
        android:text="Sleep: "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/log_age1" />

    <TextView
        android:id="@+id/log_sleepData1"
        android:layout_width="294dp"
        android:layout_height="57dp"
        android:layout_marginTop="10dp"
        android:text="Tid siden, varighed, hovedside"
        app:layout_constraintTop_toBottomOf="@+id/log_sleep1"
        tools:layout_editor_absoluteX="46dp" />

    <TextView
        android:id="@+id/log_mad1"
        android:layout_width="63dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:text="Feeding:"
        app:layout_constraintTop_toBottomOf="@+id/log_sleepData1"
        tools:layout_editor_absoluteX="153dp" />

    <TextView
        android:id="@+id/log_madData1"
        android:layout_width="249dp"
        android:layout_height="86dp"
        android:layout_marginTop="10dp"
        android:text="Tid siden, varighed, type"
        app:layout_constraintTop_toBottomOf="@+id/log_mad1"
        tools:layout_editor_absoluteX="46dp" />

    <TextView
        android:id="@+id/log_ble1"
        android:layout_width="87dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:text="Diapering:"
        app:layout_constraintTop_toBottomOf="@+id/log_madData1"
        tools:layout_editor_absoluteX="121dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="215dp"
        android:layout_height="73dp"
        android:layout_marginTop="10dp"
        android:text="Tid siden, type"
        app:layout_constraintTop_toBottomOf="@+id/log_ble1"
        tools:layout_editor_absoluteX="66dp" />

</android.support.constraint.ConstraintLayout>